<odoo>
  <data>
    <record id="real_estate_action" model="ir.actions.act_window">
        <field name="name">Real Estate</field>
        <field name="res_model">real_estate</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="real_estate_root" name="Real Estate">
        <menuitem id="real_estate_menu" name="Menu" action="real_estate_action">
        
        </menuitem>
    </menuitem>
    
    <record id="real_estate_view_form" model="ir.ui.view">
        <field name="name">real_estate.form</field>
        <field name="model">real_estate</field>
        <field name="arch" type="xml">
            <form string="Real Estate">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="manager"/>
                            <field name="activate"/>
                        </group>
                        <group>
                            <field name="phone"/>
                            <field name="email"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Description">
                            <field name="description"/>
                        </page>
                        <page string="Buyers">
                            <field name="buyers"/>
                        </page>
                        <page string="Offers">
                            <field name="offers"/>

                        </page>                     
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>

    <record id="real_estate_view_tree" model="ir.ui.view">
        <field name="name">real_estate.list</field>
        <field name="model">real_estate</field>
        <field name="arch" type="xml">
            <list string="Channel">
                <field name="name"/>
                <field name="phone"/>
                <field name="email"/>
            </list>
        </field>
    </record>
  </data>
</odoo>
